    "express-session": "^1.18.0",
    "@godaddy/terminus": "^4.12.1",
    "minimist": "^1.2.8",
    "jsonwebtoken": "^9.0.2",
    "jaeger-client": "^3.19.0",
    "cors": "^2.8.5",
    "fluent-ffmpeg": "^2.1.2",
    "node-media-server": "^2.6.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
