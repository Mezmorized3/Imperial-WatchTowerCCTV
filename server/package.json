
{
  "name": "imperial-server",
  "version": "1.0.0",
  "description": "Imperial Server for Security Camera Scanner",
  "main": "imperialServer.js",
  "scripts": {
    "start": "node imperialServer.js",
    "start:dev": "node imperialServer.js --security-level=normal --log-level=debug",
